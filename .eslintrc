{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "max-len": "off",
    "no-unused-vars": "off",
    "new-cap": "off",
    "require-jsdoc": "off",
    "default-case": "off",
    "no-trailing-spaces": "off",
    "prefer-const": "off",
    "eol-last": "off",
    "no-unused-expressions": "off",
    "quotes": "off",
    "consistent-return": "off",
    "no-param-reassign": "off",
    "space-before-function-paren": "off",
    "no-empty": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}